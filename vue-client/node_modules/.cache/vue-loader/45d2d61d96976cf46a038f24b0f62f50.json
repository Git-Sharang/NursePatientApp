{"remainingRequest":"F:\\For Github\\NursePatientApp\\vue-client\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!F:\\For Github\\NursePatientApp\\vue-client\\src\\views\\nurse\\PatientInfoLog.vue?vue&type=style&index=0&id=32e46846&lang=scss&scoped=true&","dependencies":[{"path":"F:\\For Github\\NursePatientApp\\vue-client\\src\\views\\nurse\\PatientInfoLog.vue","mtime":1617734124000},{"path":"F:\\For Github\\NursePatientApp\\vue-client\\node_modules\\css-loader\\dist\\cjs.js","mtime":1617734124000},{"path":"F:\\For Github\\NursePatientApp\\vue-client\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1617734124000},{"path":"F:\\For Github\\NursePatientApp\\vue-client\\node_modules\\postcss-loader\\src\\index.js","mtime":1617734124000},{"path":"F:\\For Github\\NursePatientApp\\vue-client\\node_modules\\sass-loader\\dist\\cjs.js","mtime":1617734124000},{"path":"F:\\For Github\\NursePatientApp\\vue-client\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1617734124000},{"path":"F:\\For Github\\NursePatientApp\\vue-client\\node_modules\\vue-loader\\lib\\index.js","mtime":1617734124000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["PatientInfoLog.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyHA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"PatientInfoLog.vue","sourceRoot":"src/views/nurse","sourcesContent":["<template>\n  <div id=\"patientInfoLog\">\n    <Header />\n    <div class=\"container-fluid\">\n      <div class=\"row mt-5\">\n        <div class=\"col-10 offset-1\">\n          <div class=\"card\">\n            <div class=\"card-body\">\n              <div class=\"card-text\">\n                <div class=\"row\">\n                  <h5 class=\"col ms-2 mt-3\">List of Patients</h5>\n                </div>\n                <div class=\"row\">\n                  <div class=\"col mt-1 mb-2\">\n                    <table class=\"table table-hover\">\n                      <thead>\n                        <tr>\n                          <th>OHIP ID</th>\n                          <th>Name</th>\n                          <th>Phone #</th>\n                          <th>Actions</th>\n                        </tr>\n                      </thead>\n                      <tbody>\n                        <tr v-for=\"item in patientList\" :key=\"item.id\">\n                          <td v-for=\"data in item\" :key=\"data.id\">\n                            {{ data.ohipId }}\n                          </td>\n                          <td v-for=\"data in item\" :key=\"data.id\">\n                            {{ data.fullName }}\n                          </td>\n                          <td v-for=\"data in item\" :key=\"data.id\">\n                            {{ data.phoneNumber }}\n                          </td>\n                          <td v-for=\"data in item\" :key=\"data.id\">\n                            <button\n                              class=\"btn btn-secondary btn-sm\"\n                              v-on:click=\"dailyLog(data)\"\n                            >\n                              View\n                            </button>\n                          </td>\n                        </tr>\n                      </tbody>\n                    </table>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nimport Cookies from \"vue-cookies\";\nimport router from \"../../router/index\";\nimport Header from \"../../components/Header\";\n\nexport default {\n  name: \"PatientInfoLog\",\n  data() {\n    return {\n      patientList: [],\n      cookie: \"\",\n    };\n  },\n  metaInfo() {\n    return {\n      title: \"Patient Log\",\n    };\n  },\n  components: {\n    Header,\n  },\n  mounted() {\n    var check = Cookies.get(\"screen\");\n    if (check === \"auth\") {\n      Swal.fire({\n        icon: \"error\",\n        title: \"Login Again\",\n        text: `Sorry we don't recognize you.`,\n      }).then((res) => {\n        if (res) {\n          router.replace(\"/nurseHome\");\n        }\n      });\n    } else {\n      console.log(\"Nurse Dashboard is logged in\");\n      console.log(\"JWT Payload Username: \", Cookies.get(\"screen\"));\n      this.listOfPatients();\n    }\n  },\n  methods: {\n    listOfPatients() {\n      var self = this;\n      axios\n        .get(\"http://localhost:5000/api/nurses/allPatients\")\n        .then(function (res) {\n          console.log(\"All Patients:\", res.data);\n          var list = res.data;\n          self.patientList.push(list);\n        })\n        .catch(function (err) {\n          console.log(err);\n        });\n    },\n    dailyLog(data) {\n      var id = data.id;\n      console.log(\"Passing id:\", data.id);\n      router.replace(\"/dailyPatientInfo/\" + id);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n#patientInfoLog {\n  background: linear-gradient(180.51deg, #9346e0 0.73%, #e357c5 99.56%);\n  height: 100vh;\n\n  .card {\n    background: rgba(255, 255, 255, 0.5);\n    border-radius: 30px;\n    box-shadow: 0 25px 50px rgba (0, 0, 0, 0.1) !important;\n\n    thead,\n    tbody {\n      border: none;\n    }\n  }\n}\n</style>\n"]}]}